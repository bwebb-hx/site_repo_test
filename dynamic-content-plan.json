    "optionalElements": [
      "backgroundImage",
      "newsArticles",
      "metaDescription",
      "footerLinks"
    ],
    "arrayElements": [
      "newsArticles",
      "breadcrumbItems",
      "footerLinks"
    ],
    "selectorElements": [
      "headerComponent",
      "breadcrumbComponent", 
      "pageTitleComponent",
      "newsItemComponent",
      "footerComponent"
    ]
  }
