  "conditionalContent": {
    "optionalSections": [
      {
        "sectionName": "backgroundImage",
        "condition": "isNotNull(backgroundImage)",
        "webrelease2Implementation": "<wr-if condition=\"isNotNull(backgroundImage)\">%backgroundImageComponent.selectedValue().generateText()%</wr-if>",
        "description": "Background image is optional - page works without it",
        "fallbackBehavior": "Clean layout without background decoration"
      },
      {
        "sectionName": "newsArticlesList",
        "condition": "count(newsArticles) > 0",
        "webrelease2Implementation": "<wr-if condition=\"count(newsArticles) > 0\"><wr-for list=\"newsArticles\" variable=\"article\">%newsItemComponent.selectedValue().generateText()%</wr-for></wr-if>",
        "description": "News list only displays if articles exist",
        "fallbackBehavior": "Show 'No articles available' message"
      },
      {
        "sectionName": "articleThumbnails",
        "condition": "isNotNull(article.articleImage)",
        "webrelease2Implementation": "<wr-if condition=\"isNotNull(article.articleImage)\"><img src=\"%article.articleImage%\" alt=\"%article.articleTitle%\" /></wr-if>",
        "description": "Article thumbnails are optional per article",
        "fallbackBehavior": "Text-only article display"
      },
      {
        "sectionName": "footerNavigation",
        "condition": "count(footerLinks) > 0",
        "webrelease2Implementation": "<wr-if condition=\"count(footerLinks) > 0\"><!-- footer nav links --></wr-if>",
        "description": "Footer navigation links are optional",
        "fallbackBehavior": "Footer shows only copyright"
      }
    ],
    "conditionalStates": [
      {
        "stateName": "featuredArticles",
        "condition": "article.featured == true",
        "webrelease2Implementation": "<wr-if condition=\"article.featured == true\"><!-- featured styling --></wr-if>",
        "description": "Articles can be marked as featured for special styling",
        "visualDifference": "Different background color or border"
      },
      {
        "stateName": "emptyState",
        "condition": "count(newsArticles) == 0",
        "webrelease2Implementation": "<wr-if condition=\"count(newsArticles) == 0\"><p class=\"empty-state\">現在、お知らせはありません。</p></wr-if>",
        "description": "Display message when no news articles exist",
        "message": "現在、お知らせはありません。"
      }
    ]
  },
  "loopContent": {
    "primaryLoops": [
      {
        "loopName": "newsArticlesList",
        "elementName": "newsArticles",
        "webrelease2Implementation": "<wr-for list=\"newsArticles\" variable=\"article\" count=\"i\">%newsItemComponent.selectedValue().generateText()%</wr-for>",
        "itemVariable": "article",
        "counterVariable": "i",
        "description": "Main loop for displaying news articles",
        "itemStructure": {
          "articleTitle": "%article.articleTitle%",
          "articleDate": "%formatDate(article.articleDate, \"yyyy年MM月dd日\")%",
          "articleSummary": "%article.articleSummary%",
          "articleUrl": "%article.articleUrl%",
          "articleCategory": "%article.articleCategory%",
          "articleImage": "%article.articleImage%",
          "featured": "%article.featured%"
        },
        "sortingOptions": {
          "byDate": "Sort by article date (newest first)",
          "byCategory": "Group by category",
          "featuredFirst": "Show featured articles first"
        }
      },
      {
        "loopName": "breadcrumbPath",
        "elementName": "breadcrumbItems",
        "webrelease2Implementation": "<wr-for list=\"breadcrumbItems\" variable=\"crumb\" count=\"i\"><wr-if condition=\"i > 1\">%arrowIconComponent.selectedValue().generateText()%</wr-if><a href=\"%crumb.url%\">%crumb.text%</a></wr-for>",
        "itemVariable": "crumb",
        "counterVariable": "i",
        "description": "Loop for building breadcrumb navigation path",
        "itemStructure": {
          "text": "%crumb.text%",
          "url": "%crumb.url%",
          "current": "%crumb.current%"
        },
        "conditionalLogic": "Add arrow separator between items (not before first item)"
      }
    ],
    "utilityLoops": [
      {
        "loopName": "footerDecorativeLines",
        "elementName": "decorativeLines",
        "webrelease2Implementation": "<wr-for times=\"4\" variable=\"line\" count=\"i\"><div class=\"decorative-line\" style=\"opacity: %0.2 * i%\"></div></wr-for>",
        "description": "Create decorative lines with increasing opacity",
        "usage": "Footer decoration with 4 lines at different opacities"
      }
    ],
    "paginationSupport": [
      {
        "feature": "articleLimiting",
        "implementation": "<wr-for list=\"newsArticles\" variable=\"article\" count=\"i\"><wr-break condition=\"i > articlesPerPage\"/>%newsItemComponent.selectedValue().generateText()%</wr-for>",
        "elementName": "articlesPerPage",
        "description": "Limit number of articles displayed per page",
        "defaultValue": 10
      },
      {
        "feature": "loadMoreSupport",
        "implementation": "JavaScript-based load more functionality",
        "description": "Progressive loading of additional articles",
        "futureEnhancement": true
      }
    ]
  },
